    <script src="~/reveal/lib/jquery/jquery-migrate.min.js"></script>
    <script src="~/reveal/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/reveal/lib/easing/easing.min.js"></script>
