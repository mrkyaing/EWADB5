    <script src="~/reveal/js/main.js"></script>
    <!--sweetalert2-->
    <script src="~/lib/sweetalert2/sweetalert2.min.js"></script>
    <!--toastr-->
    <script src="~/lib/toastr.js/toastr.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
